    "utf-8-validate": "^5.0.9"
  },
  "devDependencies": {
    "@types/luxon": "^3.0.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "dotenv": "^16.0.2",
    "esbuild": "^0.15.7",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.23.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.8.3"
  }
